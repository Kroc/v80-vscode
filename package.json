{
	"name"			: "vscode-v80",
	"publisher"		: "KrocCamen",
	"displayName"	: "v80",
	"description"	: "Support for v80 assembler syntax.",
	"license"		: "MIT",
	"version"		: "0.1.2",
	"engines"		: { "vscode": "^1.90.0" },
	"icon"			: "icon.png",
	"galleryBanner"	: { "color": "#000d05", "theme": "dark" },
	"author"		: {
		"name"		: "Kroc Camen",
		"email"		: "kroc@camendesign.com",
		"url"		: "https://camendesign.com"
	},
	"repository" 	: {
		"type"		: "git",
		"url"		: "https://github.com/Kroc/vscode-v80"
	},
	"categories"	: [ "Programming Languages" ],
